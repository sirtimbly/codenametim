{
    "compilerOptions": {
        "outDir": "./built/",
        "allowJs": true,
        "sourceMap": true,
        "strict": false,
        "noImplicitReturns": true,
        "allowSyntheticDefaultImports": true,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5"
    },
    "include": ["src/**/*.ts","src/**/*.vue"]
}
